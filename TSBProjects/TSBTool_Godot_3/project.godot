; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="TSBTool_Godot_3"
run/main_scene="res://Forms/main_panel.tscn"
config/icon="res://Images/52.svg"

[autoload]

Globals="*res://Utilities/Globals.gd"
SceneManager="*res://Utilities/SceneManager.gd"

[display]

window/size/height=648
window/energy_saving/keep_screen_on=false

[gui]

common/drop_mouse_on_gui_input_disabled=true

[mono]

runtime/unhandled_exception_policy=1
project/assembly_name="TSBTool_Godot_3"
export/export_assemblies_inside_pck=false

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
